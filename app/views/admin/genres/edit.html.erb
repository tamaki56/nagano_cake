<div class="container px-5">
  <div class="row">
    <div class="col-lg-5">
      <h1>ジャンル編集</h1>
      
      <%= form_with model: @genre, url: admin_genre_path(@genre.id), method: :patch, local: true do |f| %>
        <div class="row mb-3">
          <label for="nputEmail3" class="col-3 col-form-label">ジャンル名</label>
          <div class="col-9">
          <%= f.text_field :name %>
          <%=f.submit '変更を保存',class: "btn btn-success" %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
